    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Evenements du jour</span>
          <p>       <% @eventsoftheday.each do |event| %>
              <span><%= event.eventtype %></span><br>
              <span><%= event.eventdate %></span><br>
              <span><%= event.eventbegin %></span><br>
              <span><%= event.eventend %></span><br>
              <span><%= event.description1 %></span><br>
              <span><%= event.description2 %></span><br>
              <span><%= event.location %></span><br>
              <% end %></p>
        </div>
        <div class="card-action">
          <a href="#">This is a link</a>
          <a href="#">This is a link</a>
        </div>
      </div>
    </div>